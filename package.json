{
  "name": "portfolio",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "wrangler dev",
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types",
    "prepare": "svelte-kit sync || echo ''",
    "content:build": "content-collections build",
    "check": "content-collections build && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "content-collections build && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "devDependencies": {
    "@content-collections/cli": "^0.1.9",
    "@content-collections/core": "^0.14.1",
    "@content-collections/vite": "^0.2.9",
    "@eslint/js": "^10.0.1",
    "@sveltejs/adapter-cloudflare": "^7.2.8",
    "@sveltejs/kit": "^2.53.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.2.0",
    "@types/three": "^0.183.1",
    "eslint": "^10.0.1",
    "eslint-plugin-svelte": "^3.15.0",
    "globals": "^17.3.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rehype-pretty-code": "^0.14.1",
    "shiki": "^3.22.0",
    "svelte": "^5.53.1",
    "svelte-check": "^4.4.3",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "wrangler": "^4.67.0"
  },
  "dependencies": {
    "@threlte/core": "^8.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.575.0",
    "tailwind-merge": "^3.5.0",
    "three": "^0.183.1",
    "varsel": "^0.5.4",
    "zod": "^4.3.6"
  }
}
